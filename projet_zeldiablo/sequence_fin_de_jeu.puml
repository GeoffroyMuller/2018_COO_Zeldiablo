@startuml

Participant "main : Main " as m
create "zeldiablojeu : ZelDiabloJeu" as j
Participant "aventurier : Aventurier" as av
Participant "salle : Salles" as s
Participant "coordonne: Coordonne" as c
PArticipant "grille : Case[][]" as g
Participant "case : Case" as ca
Participant "victoire : Boolean" as v
m->j 
m->j : evoluer()
activate j
j->av : getCoor(coor)

activate av
av--> j :Coordonnee co
j->av : getSalle()
av-->j : Salle salle

deactivate av

j-> c : getX()
activate c
c-->j : int x

j-> c : getY()

c-->j : int y

deactivate c
j-> s : getGrille[][]
activate s

s-->j : grille

deactivate s
loop : for sur i et j
j->g : grille[i][j]
activate g
g-->j : Case case
deactivate g

j->ca :getType()
ca -->j  :String type

alt type.equals("portailfin")
end
end
j->v : set True
activate v
deactivate v
deactivate j

@enduml