@startuml
Participant "p : Potion" as p
Participant "av : Aventurier" as av
->p : effetItem(av : Aventurier)
activate p

alt : av.getBaseVie()+av.getStat().getResistance()<(av.getVie()+50)
p-> av :av.setVie(av.getBaseVie()+av.getStat().getResistance())
p<--av
else
p-> av : av.setVie(av.getVie()+50)
p<--av
end

<-- p
deactivate p
@enduml